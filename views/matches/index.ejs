<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
      <th>Home Team</th>
      <th>Score</th>
      <th>Away Team</th>
    </tr>
  </thead>
  <tbody>
    <% matches.forEach(function(m) { %>
      <tr>
        <td><%= m.homeTeam %></td>
        <td><%= m.scoreOne %> - <%= m.scoreTwo %> </td>
        <td><%= m.awayTeam %></td>
        <td><a href="/matches/<%= m._id %>">DISCUSSION</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>